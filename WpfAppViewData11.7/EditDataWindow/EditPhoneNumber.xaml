<Window x:Class="WpfAppViewData11._7.EditPhoneNumber"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfAppViewData11._7"
        mc:Ignorable="d"
        Title="EditPhoneNumber" Height="150" Width="300">
    <Grid>
        <Label Content="Введите номер телефона" HorizontalAlignment="Left" Margin="48,10,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="newPhoneNumbersTextBox" HorizontalAlignment="Center" Margin="0,49,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="148"/>
        <Button x:Name="SaveNumberButton" Content="Сохранить" HorizontalAlignment="Left" Margin="199,90,0,0" VerticalAlignment="Top" Click="SaveNumber_Click"/>

    </Grid>
</Window>
